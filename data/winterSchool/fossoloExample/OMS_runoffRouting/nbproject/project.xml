<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.ant.freeform</type>
    <configuration>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/1">
            <name>graph</name>
        </general-data>
        <general-data xmlns="http://www.netbeans.org/ns/freeform-project/2">
            <!-- Do not use Project Properties customizer when editing this file manually. -->
            <name>graph</name>
            <properties>
                <property-file>.oms/project.properties</property-file>
            </properties>
            <folders>
                <source-folder>
                    <label>Simulations</label>
                    <type>groovy</type>
                    <location>simulation</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>graph</label>
                    <location>.</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Sources</label>
                    <type>java</type>
                    <location>src</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Data</label>
                    <type>java</type>
                    <location>data</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Simulations</label>
                    <type>java</type>
                    <location>simulation</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
                <source-folder>
                    <label>Output</label>
                    <type>java</type>
                    <location>output</location>
                    <encoding>UTF-8</encoding>
                </source-folder>
            </folders>
            <ide-actions>
                <action name="build">
                    <target>jar</target>
                </action>
                <action name="clean">
                    <target>clean</target>
                </action>
                <action name="rebuild">
                    <target>clean</target>
                    <target>jar</target>
                </action>
                <action name="run.single">
                    <script>build.xml</script>
                    <target>run-selected-file</target>
                    <context>
                        <property>run.class</property>
                        <folder>simulation</folder>
                        <pattern>\.(sim|luca|esp|fast|groovy|ps|jgt)$</pattern>
                        <format>absolute-path</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="compile.single">
                    <script>build.xml</script>
                    <target>compile-selected-files-in-src</target>
                    <context>
                        <property>files</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>relative-path</format>
                        <arity>
                            <separated-files>,</separated-files>
                        </arity>
                    </context>
                </action>
                <action name="run.single">
                    <script>build.xml</script>
                    <target>run-selected-file-in-src</target>
                    <context>
                        <property>run.class</property>
                        <folder>src</folder>
                        <pattern>\.java$</pattern>
                        <format>java-name</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
                <action name="debug.single">
                    <script>build.xml</script>
                    <target>debug-selected-file</target>
                    <context>
                        <property>run.class</property>
                        <folder>simulation</folder>
                        <pattern>\.(java|sim)$</pattern>
                        <format>absolute-path</format>
                        <arity>
                            <one-file-only/>
                        </arity>
                    </context>
                </action>
            </ide-actions>
            <view>
                <items>
                    <source-folder style="packages">
                        <label>Sources</label>
                        <location>src</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>Data</label>
                        <location>data</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>Simulations</label>
                        <location>simulation</location>
                    </source-folder>
                    <source-folder style="packages">
                        <label>Output</label>
                        <location>output</location>
                    </source-folder>
                    <source-file>
                        <location>build.xml</location>
                    </source-file>
                    <source-file>
                        <label>Project Properties</label>
                        <location>.oms/project.properties</location>
                    </source-file>
                </items>
                <context-menu>
                    <ide-action name="build"/>
                    <ide-action name="rebuild"/>
                    <ide-action name="clean"/>
                    <separator/>
                    <action>
                        <script>build.xml</script>
                        <label>Clean Output Data</label>
                        <target>clean-output</target>
                    </action>
                    <action>
                        <script>build.xml</script>
                        <label>Zip Project</label>
                        <target>zip</target>
                    </action>
                </context-menu>
            </view>
            <subprojects/>
        </general-data>
        <java-data xmlns="http://www.netbeans.org/ns/freeform-project-java/3">
            <compilation-unit>
                <package-root>src</package-root>
                <classpath mode="compile">lib/GEOframeUtils.jar:lib/HydroGeoEntities.jar:lib/commons-math3-3.6.1.jar:lib/jswmm-0.2.jar:lib/oms-all.jar</classpath>
                <source-level>1.6</source-level>
            </compilation-unit>
        </java-data>
    </configuration>
</project>
